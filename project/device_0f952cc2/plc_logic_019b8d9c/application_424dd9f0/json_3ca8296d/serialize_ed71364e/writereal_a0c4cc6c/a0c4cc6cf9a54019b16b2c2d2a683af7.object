{"payload":{"meta":{"Graph":{"@Type":"81297157","@Value":{"Guid":"(Guid)a0c4cc6c-f9a5-4019-b16b-2c2d2a683af7","ParentGuid":"(Guid)424dd9f0-284c-44c2-ab5a-30cc48b07b83","Name":"(string)WriteReal","Properties":{"@Type":"2c41fa04:IDictionary","@Value":[{"@Key":"(Guid)829a18f2-c514-4f6e-9634-1df173429203","@Value":{"@Type":"829a18f2","@Value":{"ParentObjects":{"@Type":"fa2ee218:IDictionary","@Value":[{"@Key":"(Guid)d9b2b2cc-ea99-4c3b-aa42-1e5c49e65b84","@Value":"(Guid)ed71364e-7b0b-4a77-a6ed-5657ed305f93"}]}}}}]},"TypeGuid":"(Guid)6f9dac99-8de1-4efc-8465-68ac443b7d08","EmbeddedTypeGuids":{"@Type":"[Guid]","@Value":["(Guid)a9ed5b7e-75c5-4651-af16-d2c27e98cb94","(Guid)3b83b776-fb25-43b8-99f2-3c507c9143fc"]}}},"TypeInfos":{"2c41fa04":"{2c41fa04-1834-41c1-816e-303c7aa2c05b}","81297157":"{81297157-7ec9-45ce-845e-84cab2b88ade}","829a18f2":"{829a18f2-c514-4f6e-9634-1df173429203}","fa2ee218":"{fa2ee218-a39b-4b6d-b249-49dbddbd168a}","Guid":"System.Guid","string":"System.String"}},"object":{"Graph":{"@Type":"6f9dac99","@Value":{"SpecialFunc":{"@Type":"0db3d7bb:Enum","@Value":"None"},"Implementation":{"@Type":"3b83b776","@Value":{"TextDocument":{"@Type":"f3878285","@Value":{"TextBlobForSerialisation":"(string)BytesWritten := 0;\nIsNeg := FALSE;\n\nIF Value < 0.0 THEN\n    IsNeg := TRUE;\n    Value := -Value;\nEND_IF;\n\nIntPart  := TRUNC(Value);\nFracPart := Value - TO_REAL(IntPart);\n\n// Write minus\nIF IsNeg THEN\n    WriteByte(Value := 45, pDest := pDest, Offset := Offset);\n    BytesWritten := BytesWritten + 1;\nEND_IF;\n\n// Write integer part\nBytesWritten := BytesWritten + WriteDint(Value := IntPart, pDest := pDest, Offset := Offset);\n\n// Write decimal point\nWriteByte(Value := 46, pDest := pDest, Offset := Offset);  // '.'\nBytesWritten := BytesWritten + 1;\n\n// Extract fractional digits (up to 6 decimal places)\nFracDigits := 0;\nFOR i := 0 TO 5 DO\n    FracPart := FracPart * 10.0;\n    Digit := TRUNC(FracPart);\n    FracBuf[i] := TO_BYTE(Digit) + 48;\n    FracPart := FracPart - TO_REAL(Digit);\n    FracDigits := FracDigits + 1;\nEND_FOR;\n\n// Trim trailing zeros but keep at least one decimal digit\nWHILE (FracDigits > 1) AND (FracBuf[FracDigits - 1] = 48) DO\n    FracDigits := FracDigits - 1;\nEND_WHILE;\n\n// Write fractional digits\nFOR i := 0 TO FracDigits - 1 DO\n    WriteByte(Value := FracBuf[i], pDest := pDest, Offset := Offset);\n    BytesWritten := BytesWritten + 1;\nEND_FOR;\n\nWriteReal := BytesWritten;","LineInfoPersistence":"(string)a0c4cc6c-f9a5-4019-b16b-2c2d2a683af7_Impl_LineIds"}}}},"Interface":{"@Type":"a9ed5b7e","@Value":{"TextDocument":{"@Type":"f3878285","@Value":{"TextBlobForSerialisation":"(string)FUNCTION WriteReal : DINT\nVAR_INPUT\n    Value : REAL;\nEND_VAR\nVAR_IN_OUT\n    pDest  : POINTER TO BYTE;\n    Offset : DINT;\nEND_VAR\nVAR\n    IntPart     : DINT;\n    FracPart    : REAL;\n    IsNeg       : BOOL;\n    FracDigits  : DINT;\n    FracBuf     : ARRAY[0..6] OF BYTE;\n    i           : DINT;\n    BytesWritten : DINT;\n    Digit       : DINT;\nEND_VAR\n","LineInfoPersistence":"(string)a0c4cc6c-f9a5-4019-b16b-2c2d2a683af7_Decl_LineIds"}}}},"UniqueIdGenerator":"(string)68","POULevel":{"@Type":"8e575c5b:Enum","@Value":"Standard"},"ChildObjectGuids":{"@Type":"ArrayList:IList","@Value":[]},"AddAttributeSubsequent":"(bool)False"}},"TypeInfos":{"0db3d7bb":"{0db3d7bb-cde0-4416-9a7b-ce49a0124323}","3b83b776":"{3b83b776-fb25-43b8-99f2-3c507c9143fc}","6f9dac99":"{6f9dac99-8de1-4efc-8465-68ac443b7d08}","8e575c5b":"{8e575c5b-1d37-49c6-941b-5c0ec7874787}","a9ed5b7e":"{a9ed5b7e-75c5-4651-af16-d2c27e98cb94}","ArrayList":"System.Collections.ArrayList","bool":"System.Boolean","f3878285":"{f3878285-8e4f-490b-bb1b-9acbb7eb04db}","string":"System.String"}}},"FormatVersion":"1.0"}